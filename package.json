  },
  "homepage": "https://intre.afialapis.com/",
  "files": [
    "lib",
    "dist"
  ],
  "main": "src/index.js",
  "publishConfig": {
    "main": "lib/index.js"
  },
  "cjs": "dist/intre.cjs.js",
  "browser": "dist/intre.umd.js",
  "module": "dist/intre.es.js",
  "scripts": {
    "lint": "eslint src",
    "test": "node test/index.js",
